{
  "name": "with-tailwindcss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true npm run build",
    "storybook": "BROWSER=Brave start-storybook -s ./public -p 6006",
    "story": "npm run storybook",
    "build-storybook": "build-storybook -o ./storybook-public/public -s ./public",
    "build-storybook-vercel": "build-storybook -o ./story-public -s ./public",
    "deploy-storybook": "npm run build-storybook && cd storybook-public && vercel --prod",
    "check-types": "tsc --noemit",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "npm run lint --fix"
  },
  "dependencies": {
    "@emotion/cache": "11.1.3",
    "@emotion/react": "11.1.5",
    "@emotion/server": "11.0.0",
    "@material-ui/core": "5.0.0-alpha.32",
    "@material-ui/lab": "5.0.0-alpha.32",
    "@material-ui/styles": "5.0.0-alpha.32",
    "deepmerge": "4.2.2",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "storybook-addon-next-router": "^2.0.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.0",
    "@storybook/addon-a11y": "^6.2.5",
    "@storybook/addon-essentials": "^6.2.5",
    "@storybook/builder-webpack5": "^6.2.5",
    "@storybook/react": "^6.2.5",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
